#!/bin/sh

[ -e "/tmp/emacs-te/server" ] && EMACS_SERVER_RUNNING=1 || EMACS_SERVER_RUNNING=0

if [ "$EMACS_SERVER_RUNNING" -eq "1" ] ; then
    emacsclient -s /tmp/emacs-te/server -c $* >/dev/null 2>&1 &disown 2>/dev/null
else
    /home/cmpitg/bin/emacs_cmpitg $* >/dev/null 2>&1 &disown 2>/dev/null
fi

